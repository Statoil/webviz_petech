{
  "name": "webviz_petech",
  "version": "0.1.0",
  "author": "Equinor ASA",
  "license": "LGPL-3.0-or-later",
  "description": "Webviz petech visualization JavaScript library",
  "main": "src/webviz_petech.js",
  "scripts": {
    "start": "webpack-dev-server --inline",
    "build": "npm-run-all --parallel build:js build:css",
    "build:js": "webpack --display-error-details --config webpack.config.js",
    "build:css": "atomizer -c \"./visualisations.acss.config.js\" -r \"./visualisations.acss.rules.js\" -n \"#webviz\" -H \"#webviz\" -R \"./src/webportal/visualisations/templates\" | postcss -c -u postcss-color-function -u autoprefixer --no-map -o \"./src/webportal/visualisations/html_resources/css/visualisations.acss.css\"",
    "test": "mocha **/*.spec.js --require @babel/register --require jsdom-global/register",
    "lint": "eslint src/*.js"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.14.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "jsverify": "^0.8.3",
    "mocha": "^5.2.0",
    "postcss-cli": "^5.0.0",
    "postcss-color-function": "^4.0.1",
    "postcss-loader": "^2.1.4",
    "postcss-nested": "^3.0.0",
    "npm-run-all": "^4.1.3",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "d3": "^4.11.0"
  }
}
